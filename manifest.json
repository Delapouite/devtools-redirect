{
    "name": "Devtools redirect",
    "version": "0.4",
    "manifest_version": 2,
    "description": "Catch and redirect resources loaded from a specific domain / path.",
    "icons": {
      "128": "img/icon-32x32.png"
    },
    
    
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "img/browser-icon-inactive.png"
    },
    
    
    "background": {"page": "background.html"},
    
    //"options_page": "options.html",
    
    "devtools_page": "devtools.html",
    
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    
    "permissions": [
      "storage",
      "background",
      "tabs",
      "webRequest",
      "webRequestBlocking",
      "http://*/*",
      "https://*/*"
    ]
    
}